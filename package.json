{
  "type": "module",
  "name": "@peterhirn/hello-simd-wasm",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Hello SIMD WebAssembly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterhirn/hello-simd-wasm.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wasm",
    "webassembly",
    "simd",
    "rust"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./fetch": "./dist/fetch.js",
    "./main.wasm": "./dist/main.wasm"
  },
  "scripts": {
    "dev": "pnpm build && node dist/index.js",
    "build": "tsc --build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vitest": "3.1.1"
  },
  "pnpm": {
    "ignoredOptionalDependencies": [
      "@pkgjs/parseargs",
      "fsevents"
    ]
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
