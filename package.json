{
  "type": "module",
  "name": "@peterhirn/hello-simd-wasm",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Hello SIMD WebAssembly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterhirn/hello-simd-wasm.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wasm",
    "webassembly",
    "simd",
    "rust"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./fetch": "./dist/fetch.js",
    "./main.wasm": "./dist/main.wasm"
  },
  "scripts": {
    "dev": "pnpm build && node dist/index.js",
    "build": "tsc --build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.12.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.7.4",
    "@vitest/coverage-v8": "2.1.2",
    "eslint": "9.12.0",
    "globals": "15.10.0",
    "prettier": "3.3.3",
    "typescript": "5.6.2",
    "typescript-eslint": "8.8.0",
    "vitest": "2.1.2"
  },
  "pnpm": {
    "ignoredOptionalDependencies": [
      "@pkgjs/parseargs",
      "fsevents"
    ]
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
